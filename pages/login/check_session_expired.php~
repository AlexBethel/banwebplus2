<?php
require_once(dirname(__FILE__)."/common_functions.php");
require_once(dirname(__FILE__)."/../pages/login/login.php");

function check_session_expired() {
	my_session_start();
	error_log(get_session_expired()*1);
	if (get_session_expired())
			return "alert[*note*]Your session has expired. You are now being redirected to the login screen.[*command*]load page[*note*]/index.php";
	else
			return "";
}

if ($s_command == "check_session_expired") {
		echo check_session_expired();
} else {
		echo "failed|bad command";
}

?>